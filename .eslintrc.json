{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-console": 0,
        "no-undef": 0,
        "semi": 0,
        "comma-dangle": 0,
        "no-param-reassign": 0,
        "max-len": 0,
        "eol-last": 0
    }
}
